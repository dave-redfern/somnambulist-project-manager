{
    "type": "project",
    "name": "somnambulist/project-manager",
    "description": "A CLI utility for helping manage a micro-services project",
    "license": "MIT",
    "require": {
        "php": "^7.3",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "pragmarx/ia-str": "~7.3",
        "somnambulist/collection": "~3.2",
        "symfony/config": "5.1.*",
        "symfony/console": "5.1.*",
        "symfony/dependency-injection": "5.1.*",
        "symfony/dotenv": "5.1.*",
        "symfony/finder": "5.1.*",
        "symfony/http-kernel": "5.1.*",
        "symfony/process": "5.1.*",
        "symfony/yaml": "5.1.*"
    },
    "require-dev": {
        "phpunit/phpunit": "~9",
        "symfony/var-dumper": "5.1.*"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "process-timeout": 3600,
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-4": {
            "Somnambulist\\ProjectManager\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Somnambulist\\ProjectManager\\Tests\\": "tests"
        }
    }
}
